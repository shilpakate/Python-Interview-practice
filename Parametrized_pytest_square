import pytest

@pytest.mark.parametrize("num,expected",
                         [
                             (1,1),
                             (2,4),
                             (3,9)

                         ])
def test_square(num,expected):
    assert num ** 2 == expected, f"Square of {num} is not 1, 4, or 9"
#
def test_sum():
    assert 1 + 2 == 3, "1 + 2 should equal 3"
